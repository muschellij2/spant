Package: spant
Type: Package
Title: MR Spectroscopy Analysis Tools
Version: 0.6.0.9000
Date: 2017-10-10
Authors@R: c(
  person("Martin", "Wilson", email = "martin@pipegrep.co.uk", role = c("cre","aut")),
  person("John", "Muschelli", role = "ctb"))
Description: Tools for reading, visualising and processing Magnetic Resonance
    Spectroscopy data.
BugReports: https://github.com/martin3141/spant/issues
License: GPL-3
RoxygenNote: 6.0.1
Roxygen: list(markdown = TRUE, roclets = c("rd", "namespace", "collate"))
LazyData: yes
Depends:
    R (>= 2.10)
Imports:
    abind,
    plyr,
    foreach,
    pracma,
    stringr,
    complexplus,
    signal,
    matrixcalc,
    minpack.lm,
    nnls,
    utils,
    graphics,
    grDevices,
    smoother,
    svd,
    tkrplot
Suggests:
    oro.nifti,
    MASS,
    fastICA,
    fields,
    mmand,
    ptw,
    viridisLite,
    viridis,
    RNifti,
    RNiftyReg,
    neurobase,
    knitr,
    magrittr,
    rmarkdown
VignetteBuilder:
    knitr
Encoding: UTF-8
